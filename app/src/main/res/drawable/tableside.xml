<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Left stroke -->
    <item android:left="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#FEFAF5" />
            <stroke
                android:width="1dp"
                android:color="#87765B" />
        </shape>
    </item>
    <!-- Right stroke -->
    <item android:right="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#FEFAF5" />
            <!-- No stroke here to make it transparent -->
        </shape>
    </item>
</layer-list>
